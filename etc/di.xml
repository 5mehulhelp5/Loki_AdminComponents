<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:ObjectManager/etc/config.xsd">

    <type name="Yireo\LokiAdminComponents\Form\Fieldset\FieldTypeProvider">
        <arguments>
            <argument name="fieldTypes" xsi:type="array">
                <item name="text" xsi:type="object">Yireo\LokiAdminComponents\Form\Fieldset\FieldType\Text</item>
                <item name="select" xsi:type="object">Yireo\LokiAdminComponents\Form\Fieldset\FieldType\Select</item>
            </argument>
        </arguments>
    </type>

    <type name="Yireo\LokiAdminComponents\Component\Form\FormRepository">
        <arguments>
            <argument name="actions" xsi:type="array">
                <item name="save" xsi:type="object">Yireo\LokiAdminComponents\Component\Form\FormRepository\SaveAction</item>
                <item name="delete" xsi:type="object">Yireo\LokiAdminComponents\Component\Form\FormRepository\DeleteAction</item>
                <item name="duplicate" xsi:type="object">Yireo\LokiAdminComponents\Component\Form\FormRepository\DuplicateAction</item>
                <item name="close" xsi:type="object">Yireo\LokiAdminComponents\Component\Form\FormRepository\CloseAction</item>
            </argument>
        </arguments>
    </type>
</config>
